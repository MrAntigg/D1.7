variable "token" {
  description = "token in yandex cloud"
  type        = string
  default     = "tour_token"
}

variable "cloud_id" {
  description = "Default cloud ID in yandex cloud"
  type        = string
  default     = "your_id"
}

variable "folder_id" {
  description = "Default folder ID in yandex cloud"
  type        = string
  default     = "your_id"
}

variable "zone" {
  description = "Default zone"
  type        = string
  default     = "ru-central1-b"
}